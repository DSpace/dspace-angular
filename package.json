    "json5": "^2.1.0",
    "jsonschema": "1.2.2",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.1",
    "ng-mocks": "^8.1.0",
    "ng2-file-upload": "1.2.1",
    "ng2-nouislider": "^1.8.2",
    "ngx-bootstrap": "^5.3.2",
    "ngx-infinite-scroll": "6.0.1",
    "ngx-moment": "^3.4.0",
    "ngx-pagination": "3.0.3",
    "ngx-sortablejs": "^3.1.4",
    "nouislider": "^11.0.0",
    "pem": "1.13.2",
    "postcss-cli": "^6.0.0",
    "reflect-metadata": "^0.1.13",
