    "raw-loader": "0.5.1",
    "rimraf": "^3.0.2",
    "script-ext-html-webpack-plugin": "2.1.4",
    "string-replace-loader": "^2.1.1",
    "terser-webpack-plugin": "^2.3.1",
    "ts-loader": "^5.2.0",
    "ts-node": "^8.8.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "1.7.2"
  }
}
