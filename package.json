    "@types/express-serve-static-core": "4.11.1",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "^2.8.6",
    "@types/js-cookie": "2.1.0",
    "@types/memory-cache": "0.2.0",
    "@types/mime": "2.0.0",
    "@types/node": "^9.4.6",
    "@types/serve-static": "1.13.1",
    "@types/uuid": "^3.4.3",
