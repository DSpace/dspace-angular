    "ngx-pagination": "3.0.3",
    "pem": "1.12.3",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.6",
    "ts-md5": "^1.2.4",
    "uuid": "^3.2.1",
    "webfontloader": "1.6.28",
    "zone.js": "0.8.20"
