{
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipDefaultLibCheck": true,
    "pretty": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "src/typings.d.ts"
    ],
    "lib": [
      "dom",
      "es6",
      "es2015",
      "es2016",
      "es2017"
    ],
    "types": [
      "node"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
